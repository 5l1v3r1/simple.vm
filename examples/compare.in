        store #1, 44
        cmp #1, 44
        jmpz equal

        store #1, "Inequal"
        print_str #1
        exit

:equal
        store #1, "EQUAL"
        print_str #1
        exit


        store #1, "Steve"
        cmp #1, "Kemp"
        jmpnz stevekemp

        store #1, "Eek string 1 compare failed - BUG?"
        print_str #1
        exit

:stevekemp
        store #1, "Steve"
        cmp #1, "Steve"
        jmpz stevekemp

        store #1, "Eek string 2 compare failed - BUG?"
        print_str #1
        exit


:stevekemp
        store #1, "All OK"
        print_str #1
        exit